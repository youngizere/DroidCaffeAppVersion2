<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.izere.droidcaffeappv2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_my_app_logo_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_my_app_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".DonutActivity"></activity>
        <activity android:name="com.DonutActivity" />
        <activity
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar"
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>